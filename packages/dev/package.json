{
  "name": "dev",
  "version": "1.0.0",
  "description": "dev",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "clean": "npx rimraf dist/",
    "copy-files": "npx copyfiles -u 1 src/**/*.vi dist/",
    "compile": "tsc -p tsconfig.json",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "nodemon"
  },
  "nx": {
    "implicitDependencies": [
      "interpreter"
    ],
    "targets": {
      "clean": {
        "inputs": [
          {
            "externalDependencies": [
              "rimraf"
            ]
          }
        ]
      },
      "copy-files": [
        "{projectRoot}/**.*.vi",
        {
          "externalDependencies": [
            "copyfiles"
          ]
        }
      ]
    }
  }
}
